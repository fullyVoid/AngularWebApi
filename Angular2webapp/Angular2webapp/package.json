    "@angular/http": "^4.2.6",
    "@angular/platform-browser": "^4.2.6",
    "@angular/platform-browser-dynamic": "^4.2.6",
    "@angular/router": "^4.2.6",

    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "jquery": "1.12.4",
    "jsnlog": "^2.24.0",
    "moment": "^2.18.1",
    "rxjs": "^5.4.2",
    "sly": "github:darsain/sly",
    "zone.js": "^0.8.12"
  },
