    "style-loader": "^0.18.2",
    "to-string-loader": "^1.1.5",
    "typescript": "~2.4.1",
    "url-loader": "^0.5.9",
    "webpack": "^2.7.0",
    "webpack-merge": "^4.1.0"
  }
